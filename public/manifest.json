{
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "version": "1.0",
    "description": "A Chrome extension",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["storage", "activeTab", "scripting",  "sidePanel"],
    
    "background": {
      "service_worker": "background.js"
    },

    "side_panel": {
    "default_path": "index.html"
  }

  
  }